{
    "env": {
        "es2021": true,
        "node": true,
        "commonjs": true,
        "browser": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign" : "off",
        "max-len":"off"
    }
}
